{
    "extends": "./base.json",
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": [
            "ESNext",
            "DOM"
        ],
        "jsx": "react-jsx",
        "module": "ESNext",
        "sourceMap": true,
        "resolveJsonModule": true,
        "noEmit": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true
    },
    "exclude": [
        "node_modules"
    ]
}